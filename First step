#include <iostream>
#include <cstdlib>
#include <ctime>

int main() {
    // Seed the random number generator
    std::srand(std::time(nullptr));

    // Generate a random number between 1 and 100
    int randomNumber = std::rand() % 100 + 1;

    // Output the random number
    std::cout << "Random number: " << randomNumber << std::endl;

    return 0;
}
